/*
这段代码可以参考 问题33的第一种解法的寻找K 的步骤来解决  进行解决
*/

class Solution {
public:
    int findMin(vector<int>& nums) {
        int left = 0;
        int right = nums.size() - 1;

        while (left < right)
        {
            int mid = left + (right - left) /2;
            if (nums[mid] <= nums[right])
            {
                right = mid;
            }
            else
            {
                left = mid + 1;
            }
        }
        return nums[left];


    }
};
